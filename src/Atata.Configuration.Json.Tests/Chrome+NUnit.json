{
  "drivers": [
    {
      "type": "Chrome",
      "options": {
        "arguments": [ "disable-extensions", "no-sandbox", "start-maximized" ],
        "additionalCapabilities": []
      }
    },
    {
      "remoteAddress": "http://127.0.0.1:4444/wd/hub",
      "desiredCapabilities": {
        "type": "Firefox",
        "capabilities": [
          {
            "capability": "cap1",
            "value": "some-val"
          }
        ]
      },
      "commandTimeout": 60
    }
  ],
  "useNUnitTestName": true,
  "logNUnitError": true,
  "logConsumers": [
    {
      "type": "Atata.NUnitTestContextLogConsumer, Atata",
      "minLevel": "Info",
      "sectionFinish": true
    }
  ]
}
