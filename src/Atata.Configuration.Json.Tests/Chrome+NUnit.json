{
  "driver": {
    "type": "Chrome",
    "options": {
      "arguments": [ "disable-extensions", "no-sandbox", "start-maximized" ],
      "additionalCapabilities": []
    }
  },
  "useNUnitTestName": true,
  "logNUnitError": true,
  "logConsumers": [
    {
      "type": "Atata.NUnitTestContextLogConsumer, Atata",
      "minLevel": "Info",
      "sectionFinish": true
    }
  ],
  "assertionExceptionType": "Atata.AssertionException, Atata",
  "retryTimeout": 10,
  "retryInterval": 0.5
}
